{
    "name": "thiagolp90/slack",
    "version": "1.0",
    "description": "Send messages with Slack API.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Developes\\Slack\\": "src/",
            "Developes\\Slack\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Developes\\Slack\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Thiago Lopes",
            "email": "thiagolp.90@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Developes\\Slack\\SlackServiceProvider"
            ],
            "aliases": {
                "Slack": "Developes\\Slack\\Facades\\Slack"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
